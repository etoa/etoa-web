{
    "name": "etoa/etoa-web",
    "description": "EtoA Portal and Login Site",
    "type": "project",
    "license": "AGPL",
    "authors": [
        {
            "name": "Nicolas Perrenoud",
            "email": "mrcage@etoa.ch"
        }
    ],
    "require": {
        "php": "^8.0.0",
        "adbario/php-dot-notation": "^3.1",
        "php-di/slim-bridge": "^3.2",
        "slim/slim": "4.*",
        "slim/psr7": "^1.5",
        "vlucas/phpdotenv": "^5.4",
        "slim/twig-view": "^3.3",
        "nesbot/carbon": "^2.61",
        "tuupola/slim-basic-auth": "^3.3",
        "bryanjhv/slim-session": "~4.0",
        "doctrine/orm": "^2.13",
        "symfony/cache": "^6.0",
        "illuminate/collections": "^9.26"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload"
        ],
        "post-update-cmd": [
            "composer dump-autoload"
        ]
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.10"
    }
}
